# For n = 250

p1 = 1; n = 250; delta = 0.025

round(true_rate_normal(p1, n, delta),7)
round(proposed_rate(delta),7)

round(true_log_error_rate(p1,n , delta),4)
round(proposed_log_error_rate(delta),4)



p1 = 1; n = 250; delta = 0.1

round(true_rate_normal(p1, n, delta),7)
round(proposed_rate(delta),7)

round(true_log_error_rate(p1,n , delta),4)
round(proposed_log_error_rate(delta),4)


